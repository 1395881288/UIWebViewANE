<extension xmlns="http://ns.adobe.com/air/extension/17.0">
  <id>com.janumedia.ane.uiwebview</id>
  <versionNumber>1.0.0</versionNumber>
  <platforms>
	<platform name="iPhone-ARM">
      <applicationDeployment>
        <nativeLibrary>libUIWebViewANE.a</nativeLibrary>
        <initializer>UIWebViewANEExtInit</initializer>
        <finalizer>UIWebViewANEExtFinal</finalizer>
      </applicationDeployment>
    </platform>  
	<platform name="Android-ARM">
      <applicationDeployment>
        <nativeLibrary>libUIWebViewANE.jar</nativeLibrary>
		<initializer>com.janumedia.ane.uiwebview.UIWebViewExtension</initializer>
        <finalizer>com.janumedia.ane.uiwebview.UIWebViewExtension</finalizer>
      </applicationDeployment>
    </platform>
	<platform name="default">
      <applicationDeployment/>
    </platform>
  </platforms>
</extension>